{
  "manifest_version": 2,
  "name": "Happy typing",
  "description": "This extension will log non focused key inputs for further use",
  "version": "1.0",
  "browser_action": {
   "default_icon": "icon.png"
  },
  "content_scripts": [
	  {
		"matches": [
		  "<all_urls>"
		],
		"js": ["jquery-1.11.3.min.js", "happy-typing.js"]
	  }
	]
}